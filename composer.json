{
	"name": "hoborglabs/commons-bundle",
	"description": "Commons bundle",
	"license": "MIT",
	"config": {
		"vendor-dir": "vendors"
	},
	"authors": [
		{
			"name": "Wojtek Oledzki",
			"email": "wojtek@hoborglabs.com",
			"homepage": "http://hoborglabs.com"
		}
	],
	"repositories": [
        { "type": "vcs", "url": "git@github.com:hoborglabs/PhabricBundle.git" }
    ],

	"require": {
		"hoborglabs/phabric-bundle": "*",
		"doctrine/common": ">2.2@stable",
		"doctrine/dbal": ">2.2@stable",
		"doctrine/orm": ">2.2@stable",

		"symfony/symfony": "2.0.15",
		"monolog/monolog": ">=1.0@stable"
	},
	"require-dev": {
		"behat/behat": "2.4.0@stable",
		"behat/behat-bundle": "1.0"
	},
	"autoload": {
		"psr-0": {
			"Hoborg\\Bundle\\CommonsBundle": "src/"
		}
	}
}
